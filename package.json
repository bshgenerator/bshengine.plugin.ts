{
  "name": "@bshsolutions/engine-plugin",
  "version": "0.0.1",
  "description": "A package for managing BSH Engine plugins <https://engine.bousalih.com>",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "bsh-plugin": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "demo": "tsx demo/demo.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "bsh-solutions",
    "bsh-engine",
    "engine-plugin",
    "typescript"
  ],
  "author": "Bousalih Hamza <hamza.bousalih.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bshgenerator/bshengine.plugin.ts.git"
  },
  "homepage": "https://engine.bousalih.com",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@bshsolutions/sdk": "latest",
    "commander": "^14.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.0.0"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  }
}
